//random # from 0 - 100
//divisible by 3 log "Fizz"
//divisible by 5 log "Buzz"
//divisible by 3 and 5 log "FizzBuzz"

let randomNumber = Math.floor(Math.random() * 100)
i = randomNumber

for (let i = 0; i < 100; i++);
if (i % 5 && i % 3 === 0){
  console.log("FizzBuzz")
} else if (i % 3 === 0){
  console.log("Fizz")
} else if (i % 5 === 0){
  console.log("Buzz")
} else {
  console.log("none of these work")
}

